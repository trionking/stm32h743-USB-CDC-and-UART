{
    "version": 4,
    "configurations": [
        {
            "name": "STM32",
            "compilerPath": "${env:STM32_CUBE_CLT_PATH}/GNU-tools-for-STM32/bin/arm-none-eabi-gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "includePath": [
                "${workspaceFolder}/Core/Inc",
                "${workspaceFolder}/Drivers/STM32H7xx_HAL_Driver/Inc",
                "${workspaceFolder}/Drivers/STM32H7xx_HAL_Driver/Inc/Legacy",
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32H7xx/Include",
                "${workspaceFolder}/Drivers/CMSIS/Include",
                "${workspaceFolder}/FATFS/Target",
                "${workspaceFolder}/FATFS/App",
                "${workspaceFolder}/USB_DEVICE/App",
                "${workspaceFolder}/USB_DEVICE/Target",
                "${workspaceFolder}/Middlewares/Third_Party/FatFs/src",
                "${workspaceFolder}/Middlewares/ST/STM32_USB_Device_Library/Core/Inc",
                "${workspaceFolder}/Middlewares/ST/STM32_USB_Device_Library/Class/CDC/Inc"
            ],
            "defines": [
                "DEBUG",
                "USE_HAL_DRIVER",
                "STM32H743xx",
                "USE_PWR_LDO_SUPPLY"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/Core",
                    "${workspaceFolder}/Drivers",
                    "${workspaceFolder}/Middlewares",
                    "${workspaceFolder}/USB_DEVICE",
                    "${workspaceFolder}/FATFS"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ]
}
